generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Model de Exemplo para o Card√°pio
model ItemCardapio {
  id         String   @id @default(uuid())
  nome       String
  descricao  String
  preco      Decimal
  categoria  String

  @@map("itens_cardapio")
}
